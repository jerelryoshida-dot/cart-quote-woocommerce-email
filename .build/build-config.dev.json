{
  "environment": "development",
  "include_dirs": [
    "../src/",
    "../templates/",
    "../assets/",
    "../tests/"
  ],
  "exclude_patterns": [
    ".build/",
    ".git/",
    ".github/",
    ".gitignore",
    ".DS_Store",
    "Thumbs.db",
    "__pycache__/",
    "*.pyc",
    "*.pyo",
    "node_modules/",
    "vendor/",
    "*.log",
    "*.tmp",
    "*.temp",
    "test-extract/",
    "test-validate/",
    "build-config.local.json",
    "build-config.prod.json",
    "*.zip",
    "dist/",
    "output/"
  ],
  "fail_on_missing": false
}
