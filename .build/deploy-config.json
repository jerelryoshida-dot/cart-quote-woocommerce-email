{
  "repository": {
    "owner": "jerelryoshida-dot",
    "name": "cart-quote-woocommerce-email",
    "dev_branch": "dev",
    "master_branch": "master"
  },
  "version": {
    "auto_increment": true,
    "increment_type": "patch",
    "files": [
      "../cart-quote-woocommerce-email.php",
      "../src/Core/Plugin.php",
      "../../cart-quote-dev-tools/tests/phpunit/bootstrap.php",
      "../README.md"
    ]
  },
  "build": {
    "create_zip": true,
    "validate_zip": true,
    "output_dir": "output"
  },
  "documentation": {
    "update_readme": true,
    "update_wiki": true,
    "wiki_pages": [
      "Update-Log.md",
      "Changelog.md"
    ]
  },
  "release": {
    "create_github_release": true,
    "attach_zip": true,
    "generate_notes": true
  },
  "git": {
    "commit_message_template": "v{version}: {changelog}",
    "require_clean_tree": false,
    "push_to_dev_first": true,
    "merge_strategy": "no-ff"
  },
  "prompts": {
    "changelog": true,
    "change_type": true,
    "additional_details": true,
    "confirm_before_push": true
  },
  "cleanup": {
    "delete_temp_zip": false,
    "delete_release_notes": true,
    "keep_build_logs": true
  }
}
