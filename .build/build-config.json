{
  "plugin_name": "cart-quote-woocommerce-email",
  "version": "1.0.14",
  "environment": "production",
  "include_dirs": [
    "../src/",
    "../templates/",
    "../assets/"
  ],
  "include_files": [
    "../cart-quote-woocommerce-email.php",
    "../readme.txt",
    "../uninstall.php"
  ],
  "exclude_patterns": [
    ".build/",
    ".git/",
    ".github/",
    ".gitignore",
    ".DS_Store",
    "Thumbs.db",
    "__pycache__/",
    "*.pyc",
    "*.pyo",
    "node_modules/",
    "vendor/",
    "*.log",
    "*.tmp",
    "*.temp",
    "test-extract/",
    "test-validate/",
    "build-config.local.json",
    "build-config.dev.json",
    "build-config.prod.json",
    "build-zip.py",
    "*.zip",
    "dist/",
    "output/"
  ],
  "required_files": [
    "../cart-quote-woocommerce-email.php",
    "../readme.txt"
  ],
  "fail_on_missing": true,
  "output_dir": "output/"
}
